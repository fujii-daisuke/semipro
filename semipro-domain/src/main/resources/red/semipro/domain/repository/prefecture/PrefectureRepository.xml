<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
    namespace="red.semipro.domain.repository.prefecture.PrefectureRepository">

    <resultMap id="prefectureResultMap" type="red.semipro.domain.model.Prefecture">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
    </resultMap>

    <select id="findAll" resultMap="prefectureResultMap">
        SELECT
            id
            , name
        FROM prefecture
        ORDER BY id
    </select>

</mapper>